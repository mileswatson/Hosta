<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style TargetType="Label" x:Key="Label">
		<Setter Property="Margin" Value="5,5,15,5" />
	</Style>

	<Style TargetType="Label" x:Key="SubHeadingLabel">
		<Setter Property="FontSize" Value="16px" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="Margin" Value="5,5,15,5" />
	</Style>

	<Style TargetType="Label" x:Key="FieldLabel">
		<Setter Property="Margin" Value="5,5,0,5" />
	</Style>

	<Style TargetType="Label" x:Key="TabHeadingLabel">
		<Setter Property="FontFamily" Value="Roboto" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="FontSize" Value="32" />
		<Setter Property="Foreground" Value="White" />
		<Setter Property="Background" Value="Green" />
		<Setter Property="Padding" Value="10,5,20,5" />
	</Style>
</ResourceDictionary>